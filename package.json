{
  "name": "gatsby-octoprint-status",
  "private": true,
  "description": "Octostatus - a lightweight octoprint status page",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy-github": "gatsby clean && gatsby build && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "gatsby": "^2.18.8",
    "gatsby-plugin-typescript": "^2.1.23",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-moment": "^0.9.7",
    "store2": "^2.10.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexleekt/gatsby-octoprint-status"
  },
  "bugs": {
    "url": "https://github.com/alexleekt/gatsby-octoprint-status/issues"
  }
}
